{
    "name": "smsg/update-indexes",
    "description": "Update Asset Indexes",
    "type": "craft-plugin",
    "version": "dev-master",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "update indexes"
    ],
    "support": {
        "docs": "https://github.com/allampraveen/craftindex/blob/master/README.md",
        "issues": "https://github.com/allampraveen/craftindex/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Praveen",
            "homepage": "smsglobal.com"
        }
    ],
  "require": {
        "craftcms/cms": "3.*"
    },
    "autoload": {
        "psr-4": {
          "smsg\\updateindexes\\": "src/"
        }
    },
    "extra": {
        "name": "Update Indexes",
        "handle": "update-indexes",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/allampraveen/craftindex/master/CHANGELOG.md",
        "class": "smsg\\updateindexes\\UpdateIndexes"
    }
}
